.tab-bar {
  position: fixed;
  bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  width: 100%;
  background-color: #fff;
  box-shadow: 0px 11px 25px 2px #C6C6C6;
}
.tab-bar .tab-list {
  padding: 20rpx;
}
.tab-bar .tab-list .tab-item .t-img {
  width: 40rpx;
  height: 40rpx;
}
.tab-bar .tab-list .tab-item .t-txt {
  font-size: 24rpx;
}
.tab-bar .tab-list .tab-item .t-txt.active {
  color: #13227a;
}
.custom-wrap {
  padding: 0;
  width: 550rpx;
  position: relative;
}
.custom-wrap .close {
  position: absolute;
  bottom: -120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.custom-wrap .title {
  overflow: hidden;
  height: 160rpx;
  padding-top: 20rpx;
  padding-left: 20rpx;
  color: #fff;
  border-radius: 10px 10px 0 0;
  box-sizing: border-box;
  background: url(data:image/jpeg;base64,);
  background-size: 100% 100%;
}
.custom-wrap .tel {
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.custom-wrap .tel .tel-btn {
  color: #FE5B07;
  border: 1px solid #FE5B07;
  display: inline-block;
  padding: 10rpx 20rpx;
  font-size: 30rpx;
  border-radius: 10rpx;
}
.custom-wrap .ewm-wrap {
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 30rpx 0;
}
.custom-wrap .ewm-wrap .ewm {
  width: 200rpx;
  margin: 40rpx 0;
}
.custom-wrap .ewm-wrap .text {
  border-top: 1px solid #eee;
  font-size: 28rpx;
  width: 100%;
  line-height: 40rpx;
  text-align: center;
}
.add {
  width: 80rpx;
  height: 80rpx;
}

